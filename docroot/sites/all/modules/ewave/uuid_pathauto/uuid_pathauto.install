<?php
/**
 * @file
 * Install, update and uninstall functions for the uuid_features module.
 *
 */

/**
 * Implements hook_enable().
 */
function uuid_pathauto_enable() {
  db_update('system')
    ->fields(array(
      'weight' => -50,
    ))
    ->condition('name', 'uuid_pathauto')
    ->condition('type', 'module')
    ->execute();
}
